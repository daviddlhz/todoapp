{
  "name": "todoapp",
  "version": "1.0.0",
  "scripts": {
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "start:backend": "cd backend && python manage.py runserver",
    "start:frontend": "cd frontend && npm run start",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "author": "daviddlhz",
  "license": "ISC"
}
